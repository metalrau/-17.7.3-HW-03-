money = input("Money =")
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
a = per_cent['ТКБ']
b = per_cent['СКБ']
c = per_cent['ВТБ']
d = per_cent['СБЕР']
t = int(money)
w = (round(a*t), round(b*t), round(c*t), round(d*t))
q = list(w)
print("deposit =", q)
print(" Максимальная сумма, которую вы можете заработать - ", max(q))
